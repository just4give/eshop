<!DOCTYPE html>
<html lang="en">
<head>

</head>
<body>
<% include ../common/header %>
<div class="container">
<h3>Hello, <%= model.firstName %></h3>
<h2 class="text-success">Your order has shipped</h2>

<div class="row">
    <label>Tracking# :</label><span><%= model.trackingNumber%></span>
</div>
<div class="row">
    <label>Carrier :</label><span><%= model.carrier%></span>
</div>
<div class="row">
    <label>Est Delivery :</label><span><%= model.estDelivery%></span>
</div>
<div class="row text-center margin-top-20">
    <a class="btn btn-success" href="" >Track Your Oder</a>
</div>


<table class="table" >

    <tbody>
    <% model.carts.forEach(function(item, index){ %>
        <tr class="row">
        <td class="col-xs-3">

            <a href="#" class="thumbnail">
                <img src="http://appstacksolutions.com/images/p2.jpg" alt="...">

            </a>

           <!-- <div class="col-xs-12">
                <img src="<%= item.product.photo.imageUrl %>">
            </div>-->


        </td>
        <td class="col-xs-9 ">
            <p><a><%= item.product.name%></a></p>
            <p ><label>Qty:</label><span> <%=item.quantity%></span></p>
            <p><label>Price:</label><span> $<%= item.quantity*item.product.price %></span></p>

    </tr>
    <% })%>


    </tbody>
</table>
</div>
</body>
</html>