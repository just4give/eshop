            <span class="input-group-btn">
              <button class="btn btn-default" type="button"   ui-sref="search({query: search.query })"><i class="fa fa-search"></i></button>
            </span>
          </div><!-- /input-group -->
        </form>

        <div class="col-xs-6 col-sm-3 " style="line-height: 40px; text-align: right">
