                bs-aside="aside" data-container="body">
              <i class=" fa fa-user fa-2x"></i>
            </a>
            <a  class="link visible-xs text-success pull-right"  style="margin-left: 10px" ng-click="$hide();loadDashaboard()" ng-show="loggedIn && user.roles.indexOf('admin')!= -1" data-template-url="modules/admin/tmpl/admin-side-menu.html"
                data-placement="left"  data-animation="am-slide-left"
                bs-aside="aside" data-container="body"><i class="fa fa-cog fa-2x"></i> </a>



